{
  "name": "@zedux/react",
  "version": "0.5.0-rc.0",
  "description": "A Molecular State Engine for React",
  "main": "./dist/zedux-react.umd.min.js",
  "module": "./dist/zedux-react.es.min.js",
  "publishConfig": {
    "registry": "https://omnistac.jfrog.io/omnistac/api/npm/npm-local/"
  },
  "types": "./dist/react/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "devtools",
    "dist"
  ],
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "rimraf dist && yarn build:prod && yarn build:dev && yarn build:devtools",
    "build:dev": "MODE=development vite build",
    "build:devtools": "PACKAGE=devtools vite build",
    "build:prod": "vite build",
    "dev": "vite",
    "prepublishOnly": "yarn build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "test": "cd ../.. && yarn test packages/react"
  },
  "keywords": [
    "atom",
    "atoms",
    "atomic",
    "boilerplate",
    "DI",
    "dependency",
    "easy",
    "flux",
    "functional",
    "immutable",
    "injection",
    "machine",
    "management",
    "memoized",
    "performant",
    "react",
    "reducer",
    "redux",
    "simple",
    "state",
    "testing",
    "zedux"
  ],
  "author": "Joshua Claunch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omnistac/zedux.git"
  },
  "bugs": {
    "url": "https://github.com/Omnistac/zedux/issues"
  },
  "exports": {
    ".": {
      "development": {
        "import": "./dist/react/zedux-react.es.js",
        "require": "./dist/react/zedux-react.umd.js",
        "default": "./dist/react/zedux-react.umd.js"
      },
      "import": "./dist/zedux-react.es.min.js",
      "require": "./dist/zedux-react.umd.min.js",
      "default": "./dist/zedux-react.umd.min.js"
    },
    "./devtools": {
      "import": "./dist/react/devtools/zedux-react-devtools.es.js",
      "require": "./dist/react/devtools/zedux-react-devtools.umd.js",
      "default": "./dist/react/devtools/zedux-react-devtools.umd.js"
    }
  },
  "homepage": "https://github.com/Omnistac/zedux#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/react-dom": "^18.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
