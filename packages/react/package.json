  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "repository": {
    "directory": "packages/react",
    "type": "git",
    "url": "https://github.com/Omnistac/zedux.git"
  },
  "scripts": {
    "build": "../../scripts/build.js",
    "lint": "eslint 'src/**/*.@(tsx|ts)' && tsc --noEmit",
    "prepublishOnly": "pnpm build",
