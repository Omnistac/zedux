{
  "name": "@zedux/react",
  "version": "0.5.3",
  "description": "A Molecular State Engine for React",
  "main": "./dist/react/zedux-react.umd.js",
  "module": "./dist/react/zedux-react.es.js",
  "types": "./dist/react/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "devtools",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && yarn build:prod && yarn build:dev",
    "build:dev": "MODE=development vite build",
    "build:prod": "vite build",
    "dev": "vite",
    "prepublishOnly": "yarn build",
    "preview": "vite preview",
    "test": "cd ../.. && yarn test packages/react --collectCoverageFrom=packages/react/src/**/*.ts"
  },
  "keywords": [
    "atom",
    "atoms",
    "atomic",
    "boilerplate",
    "composable",
    "composition",
    "DI",
    "dependency",
    "easy",
    "flexible",
    "flux",
    "functional",
    "immutable",
    "injection",
    "machine",
    "management",
    "memoized",
    "molecular",
    "performant",
    "react",
    "reducer",
    "redux",
    "simple",
    "state",
    "testable",
    "zedux"
  ],
  "author": "Joshua Claunch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omnistac/zedux.git"
  },
  "bugs": {
    "url": "https://github.com/Omnistac/zedux/issues"
  },
  "exports": {
    ".": {
      "development": {
        "import": "./dist/react/zedux-react.es.js",
        "require": "./dist/react/zedux-react.umd.js",
        "default": "./dist/react/zedux-react.umd.js"
      },
      "import": "./dist/zedux-react.es.min.js",
      "require": "./dist/zedux-react.umd.min.js",
      "default": "./dist/zedux-react.umd.min.js"
    },
    "./devtools": {
      "import": "./dist/react/devtools/zedux-react-devtools.es.js",
      "require": "./dist/react/devtools/zedux-react-devtools.umd.js",
      "default": "./dist/react/devtools/zedux-react-devtools.umd.js"
    }
  },
  "homepage": "https://omnistac.github.io/zedux/",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/react-dom": "^18.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
